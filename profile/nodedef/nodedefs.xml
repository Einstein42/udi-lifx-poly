                </cmd>
                <cmd id="SETZ">
                    <p id="" editor="lifxzone" init="GV4" />
                </cmd>
                <cmd id="SET_HSBKDZ">
                    <p id="H" editor="lifxclr" init="GV1" />
                    <p id="S" editor="lifxclr" init="GV2" />
                    <p id="B" editor="lifxclr" init="GV3" />
